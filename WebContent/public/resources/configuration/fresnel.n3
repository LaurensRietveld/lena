ev:Zmod a fresnel:Lens;
	fresnel:purpose fresnel:defaultLens;
	fresnel:classLensDomain ev:Zmod;
	fresnel:showProperties (ev:application_of
				 	ev:has_rendering
					ev:was_measured_in) ;
	fresnel:group :evGroup.


  	
ev:ZmodResolution a fresnel:Lens;
	fresnel:purpose fresnel:defaultLens;
	fresnel:classLensDomain ev:ZmodResolution;
	fresnel:showProperties (ev:has_end_frequency
					ev:has_start_frequency
					ev:has_frequency_range
					ev:has_sample_rate
					ev:has_speed_range_end
					ev:has_speed_range_start
					ev:link) ;
fresnel:group :evGroup. 


