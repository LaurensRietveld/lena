ev:ZmodS a fresnel:Lens;
	fresnel:purpose fresnel:defaultLens ;
	fresnel:instanceLensDomain "SELECT ?trim WHERE { ?trim rdf:type <http://kmi.open.ac.uk/RR/modules/vib.owl#Zmod> }"^^fresnel:sparqlSelector ;
	fresnel:showProperties (ev:application_of
					ev:was_measured_in						
					[fresnel:property ev:has_rendering ;
                               fresnel:sublens ev:ZmodResolution]

	) ;
	fresnel:group :evGroup .

	


